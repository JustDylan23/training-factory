{% extends 'content_base.html.twig' %}

{% block content_body %}
    {{ form_start(form, {attr: {'novalidate': 'novalidate', 'autocomplete': 'off'}}) }}
    {{ form_widget(form) }}
    <button class="btn btn-primary mt-3">Save</button>
    {{ form_end(form) }}
{% endblock %}

{% block stylesheets %}
    {{ parent() }}
    <link rel="stylesheet" href="{{ asset('css/form.css') }}">
{% endblock %}
